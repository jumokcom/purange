# 데이터베이스 연결 설정
# PostgreSQL 연결 문자열 형식: postgresql://username:password@host:port/database
DATABASE_URL="postgresql://user:password@localhost:5432/purange"

# JWT 인증 설정
# JWT 토큰 생성에 사용되는 비밀 키
JWT_SECRET="your-secret-key"

# 서버 설정
# 서버가 실행될 포트 번호
PORT=3001

# 환경 설정
# development 또는 production
NODE_ENV="development" 